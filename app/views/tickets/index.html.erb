<% @work_stations.each do |work_station| %>
  <table>
    <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
    </thead>
    <tbody>
    <tr>
      <td><%= work_station.name %></td>
      <td><%= work_station.status %></td>
      <%= link_to 'edit ', edit_work_station_path(work_station) %>
      <%= button_to 'delete', work_station_path(work_station), method: :delete%>
    </tr>
    </tbody>
  </table>

<% end %>